@page "/dashboard"@using static MyVideoResume.Client.Shared.Resume.ResumePreviewComponent@inherits BaseComponent<PageTitle>MyVideoResu.ME - Dashboard</PageTitle><RadzenRow>    <RadzenColumn Size="11">        <CreateResumeOptionsComponent ResumeCreated=@UploadCompletedHandler></CreateResumeOptionsComponent>        <RadzenText Text="Dashboard" TextStyle="Radzen.Blazor.TextStyle.DisplayH2" TagName="Radzen.Blazor.TagName.Auto"></RadzenText>        <RadzenCard>            <RadzenRow>                <AuthorizeView>                    <Authorized>                        @if (ResumeList == null || ResumeList.Count == 0)
                        {                            <RadzenText Text="You do not have any resumes created. Please choose to create or upload a resume."></RadzenText>                        }
                        else
                        {                            <ResumeDisplayPreviewComponent DeleteCompleted=@DeleteCompletedHandler ResumeItems=@ResumeList></ResumeDisplayPreviewComponent>                        }                    </Authorized>                    <NotAuthorized>                        <UnAuthorizedComponent Path=@Paths.Dashboard_View></UnAuthorizedComponent>                    </NotAuthorized>                </AuthorizeView>            </RadzenRow>        </RadzenCard>    </RadzenColumn></RadzenRow>